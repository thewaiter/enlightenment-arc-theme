         desc { "clicked";
            inherit: "default";
            visible: 1;
            color_class: "ilist_item_base_selected";
         }
         desc { "pressed";
            inherit: "default" ;
            color_class: "button_bg_pressed";
            visible: 1;
         }
      }
      rect { "icon_clip";
         desc { "default";
         }
         desc { "disabled";
            inherit: "default";
            color: 255 255 255 64;
