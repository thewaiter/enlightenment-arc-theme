           script {
            new s = get_int(bt_state);
            set_int(bt_state, s |= HOVER);
            eval_state(s);
         }
      }
      program {
         signal: "mouse,out"; source: "event";
           script {
            new s = get_int(bt_state);
            set_int(bt_state, s &= ~HOVER);
            eval_state(s);
         }
      }
      program {
