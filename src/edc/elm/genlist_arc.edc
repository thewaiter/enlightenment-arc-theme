				new st[31], Float:vl;
				get_state(PART:"arrow_img_l", st, 30, vl);
				set_state(PART:"arrow_img_l", "disabled", vl);
				set_state(PART:"arrow_img_r", "disabled", vl);
			}
		}
	}
