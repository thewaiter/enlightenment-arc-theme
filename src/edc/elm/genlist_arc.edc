				inherit: "default";
				image.normal: "arrow-down";
			}
			desc { "selected" 1.0;
				inherit: "default";
            image.normal: "arrow-down";
			}
			desc { "disabled" 1.0;
				inherit: "default";
            image.normal: "arrow-down";
            color_class: "list_item_arrow_disabled";
