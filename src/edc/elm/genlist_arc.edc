		}
		rect { "arrow"; mouse;
			scale: 1;
			desc { "default";
				fixed: 1 0;
				min: 14 14;
				max: 14 14;
				aspect: 1.0 1.0;
				align: 0.0 0.5;
				// rel1.to_x: "elm.swallow.pad";
				// rel1.relative: 1.0 0.0;
				rel1.offset: -2 2;
				// rel2.to_x: "elm.swallow.pad";
				// rel2.relative: 1.0 1.0;

				rel2.offset: -2 -3;
				color: 0 0 0 0;
			}
		}
		rect { "arrow_clip_l";
			desc { "default";
				rel1.to: "arrow_img_l";
				rel2.to: "arrow_img_l";
				vis;
				link.base: "edje,state,ltr" "edje";
			}
			desc { "rtl";
				hid;
				link.base: "edje,state,rtl" "edje";
