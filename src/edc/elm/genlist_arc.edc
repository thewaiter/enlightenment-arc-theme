			desc { "default";
				FIXED_SIZE(14, 14)
				rel1.to: "arrow";
				rel2.to: "arrow";
				image.normal: "arrow-left";
            color_class: "list_item_arrow";
			}
			desc { "selected";
				inherit: "default";
            // color_class: "list_item_arrow_selected";
			}
			desc { "disabled";
				inherit: "default";
            color_class: "list_item_arrow_disabled";
			}
			desc { "default" 1.0;
				inherit: "default";
				image.normal: "arrow-down";
            color_class: "list_item_arrow";
			}
			desc { "selected" 1.0;
				inherit: "default";
				image.normal: "arrow-down";
            // color_class: "list_item_arrow_selected";
			}
			desc { "disabled" 1.0;
				inherit: "default";
				image.normal: "arrow-down";
            // color_class: "list_item_arrow_disabled";
