   IMAGE_SET("heart", 14, 14)
   IMAGE_SET("reload", 14, 14)
   script {

#define PRESSED 1
#define HOVER 2
#define EVAL_STATE_FNC(_NAME) \
      public _NAME ## _state; \
      public eval_ ## _NAME ## _state(s) { \
         new p = s & PRESSED; \
         new h = s & HOVER; \
         if (!p) { \
            if (!h) { \
               set_state(PART:#_NAME"_bg", "default", 0.0); \
               set_state(PART:#_NAME"_border", "default", 0.0); \
            } else { \
               set_state(PART:#_NAME"_bg", "hover", 0.0); \
               set_state(PART:#_NAME"_border", "hover", 0.0); \
            } \
         } else { \
            set_state(PART:#_NAME"_bg", "pressed", 0.0); \
            set_state(PART:#_NAME"_border", "pressed", 0.0); \
         } \
      }

EVAL_STATE_FNC(go_back)
EVAL_STATE_FNC(go_forward)
EVAL_STATE_FNC(go_parent)
EVAL_STATE_FNC(go_reload)
EVAL_STATE_FNC(go_fav)

   }
   parts {
      part { name: "base"; type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            rel1.to: "back";
            rel2.to: "favorite";
            fixed: 1 1;
            min: 0 36;
            max: -1 36;
         }
      }
      part { name: "e.swallow.pathbar"; type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            rel1.to_x: "base";
            rel1.relative: 1.0 0.0;
            rel1.offset: 3 4;
            rel2.offset: -4 -4;
         }
      }

#define BUTTON(_NAME, _REL, _ICON, _ACTION) \
      part { name: #_NAME"_bg"; \
         scale: 1; \
         description { state: "default" 0.0; \
            rel1.to: _REL; \
            rel2.to: _REL; \
            color_class: "button_bg"; \
            image.normal: "bg-rounded-3px@4.png"; \
            image.border: 12 12 12 12; \
            image.border_scale: 1; \
            image.border_scale_by: 0.25; \
            fill.smooth: 0; \
            visible: 0; \
            fixed: 1 1; \
         } \
         description { state: "pressed" 0.0; \
            inherit: "default" 0; \
            color_class: "button_bg_pressed"; \
            visible: 1; \
         } \
         description { state: "hover" 0.0; \
            inherit: "default" 0; \
            color_class: "button_bg"; \
            visible: 1; \
         } \
      } \
      part { name: #_NAME"_border"; \
         scale: 1; \
         description { state: "default" 0.0; \
            rel1.to: #_NAME"_bg"; \
            rel2.to: #_NAME"_bg"; \
            image.normal: "border-rounded-3px@4.png"; \
            image.border: 12 12 12 12; \
            image.border_scale: 1; \
            image.border_scale_by: 0.25; \
            image.middle: 0; \
            fixed: 1 1; \
            visible: 0; \
         } \
         description { state: "pressed" 0.0; \
            inherit: "default" 0; \
            color_class: "button_border"; \
            visible: 1; \
         } \
         description { state: "hover" 0.0; \
            inherit: "default" 0; \
            color_class: "button_border"; \
            visible: 1; \
         } \
      } \
      part { name: #_NAME"_icon"; \
         scale: 1; \
         description { state: "default" 0.0; \
            rel1.to: _REL; \
            rel2.to: _REL; \
            image.normal: _ICON; \
            color_class: "ilist_item"; \
            min: 14 14; \
            max: 14 14; \
            fixed: 1 1; \
         } \
      } \
      program { \
         signal: "mouse,down,1*"; source: _REL; \
         script { \
            new s = get_int(_NAME ## _state); s |= PRESSED; \
            set_int(_NAME ## _state, s); \
            eval_ ## _NAME ## _state(s); \
         } \
      } \
      program { \
         signal: "mouse,up,1*"; source: _REL; \
         script { \
            new s = get_int(_NAME ## _state); s &= ~PRESSED; \
            set_int(_NAME ## _state, s); \
            eval_ ## _NAME ## _state(s); \
         } \
      } \
      program { \
         signal: "mouse,in"; source: _REL; \
         script { \
            new s = get_int(_NAME ## _state); s |= HOVER; \
            set_int(_NAME ## _state, s); \
            eval_ ## _NAME ## _state(s); \
         } \
      } \
      program { \
         signal: "mouse,out"; source: _REL; \
         script { \
            new s = get_int(_NAME ## _state); s &= ~HOVER; \
            set_int(_NAME ## _state, s); \
            eval_ ## _NAME ## _state(s); \
         } \
      } \
      program { \
         signal: "mouse,clicked,1*"; source: _REL; \
         action: SIGNAL_EMIT "e,action,"_ACTION",click" ""; \
      } \

      BUTTON(go_back, "back", "arrow-left", "back")
      BUTTON(go_forward, "forward", "arrow-right", "forward")
      BUTTON(go_parent, "parent", "arrow-up", "up")
